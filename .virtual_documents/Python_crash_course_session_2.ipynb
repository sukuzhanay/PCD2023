def acierto(n_min=0, n_max=10):
    x = randint(n_min, n_max)
    fallo = True
    while fallo == True:
        print('Introduce un numero:')
        numero = int(input())
        if numero == x:
            print('Has acertado')
            fallo = False
        elif numero < x:
            print('Es mayor que ', numero)
        else:
            print('Es menor que ', numero)


acierto()






